<div class="cardRandom">
  {{#with this}}
  <img class="card-img-top-random" src="{{image_url}}" alt="">
  <div class="card-body-random">
    <h5 class="card-title">{{name}}</h5>
    <p class="card-tagline">{{tagline}}</p>
    <p class="card-text">{{description}}</p>
    <p class="card-text">{{food_pairing}}</p>
    <p class="card-text">{{brewers_tips}}</p>
  </div>
  {{/with}}
</div>